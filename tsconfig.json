{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@core/*": [
        "./src/core/*"
      ],
      "@/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
